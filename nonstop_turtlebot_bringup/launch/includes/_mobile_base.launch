<!--
  The mobile platform base.
  
  Selector for the base.
 -->
<launch>
  <arg name="base"/>
  <arg name="machine_name"/>
  <include file="$(find nonstop_turtlebot_bringup)/launch/includes/$(arg base)/_mobile_base.launch">
	<arg name="machine_name" value="$(arg machine_name)" />
  </include>
</launch>

