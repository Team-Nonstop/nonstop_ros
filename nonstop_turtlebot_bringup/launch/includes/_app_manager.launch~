<launch>
  <arg name="machine_name"/>
  <node pkg="turtlebot_app_manager" type="appmaster" name="appmaster" args="-p 11312" machine="$(arg robot_name)"/>
  <node pkg="turtlebot_app_manager" type="app_manager" name="app_manager" args="--applist $(find nonstop_turtlebot_bringup)/apps"  machine="$(arg machine_name)">
        <param name="interface_master" value="http://localhost:11312"/>
  </node>
</launch>
